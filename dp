   },
   "outputs": [],
   "source": [
    "# 设定一个阈值，考虑两个特征间有无相关性\n",
    "# correlation coefficient (pearman)\n",
    "mol_corr = mol.corr()\n",
    "spearman_corr = mol.corr(method='spearman')\n",
    "kendall_corr = mol.corr(method='kendall')\n",
    "molt_cov = np.cov(mol.to_numpy())\n",
    "#sns.heatmap(mol_corr, center=0, annot=True, cmap='YlGnBu') # take too much time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 179,
   "id": "1ee93256",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2021-10-17T02:32:37.869679Z",
